citHeader("To cite glmnet in publications use:")

citEntry(entry = "Article",
  title        = "Unified Nonconvex Penalty Estimation",
  author       = personList(as.person("Dongshin Kim"),
                   as.person("Sunghoon Kwon"),
                   as.person("Sangin Lee")),
  journal      = "NA",
  year         = "NA",
  volume       = "NA",
  number       = "NA",
  pages        = "NA",
  url          = "NA",

  textVersion  =
  paste("Dongshin Kim, Sunghoon Kwon, Sangin Lee (2010).",
        "Unified Nonconvex Penalty Estimation.",
        "Journal, vol(sub), pages.",
        "URL url.")
)